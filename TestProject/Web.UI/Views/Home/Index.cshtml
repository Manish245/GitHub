@{
    ViewBag.Title = "Home Page";
}

<script>

    var ModuleService = (function ($) {


        let ModuleServiceReturnObject = {
            GetChartData: function (callback) {
                var getUrl ='http://localhost:50832/api/Ganttchart'
                    $.ajax({
                        type: "get",
                        url: getUrl,
                        cache: false,
                        headers: {
                            'Content-Type': 'application/x-www-form-urlencoded'
                        },
                        success: function (data) {
                            callback(data);
                        },
                        error: function (xhr, textStatus, errorMessage) {
                            console.log(errorMessage);
                        }
                    });
            }
        }
        return ModuleServiceReturnObject;
    })($);

    var ModuleObject = (function ($, ModuleService) {
        let returnObject = {
             getData : function () {

                 ModuleService.GetChartData(function (response) {
                     $.each(response, function (data, Key) {
                         $('#dataView').append("Name: "+Key.Name+" , Date: "+Key.Date+"</br>");
                     });
                    
                });
            }
        }
        return returnObject;
    })($, ModuleService);

    ModuleObject.getData();
</script>
<div id="dataView"></div>